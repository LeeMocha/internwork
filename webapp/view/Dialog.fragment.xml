<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
    <SelectDialog
        id="valueHelpDialog"
        title="Buseo"
        items="{ path: 'buModel>/' }"
        search="_handleValueHelpSearch"
        confirm="_handleValueHelpClose"
        cancel="_handleValueHelpClose"
        rememberSelections="true"
        multiSelect="true">
        <StandardListItem
            iconInset="false"
            title="{buModel>Bukey}"
            description="{buModel>Butxt}" 
            />
    </SelectDialog>
</core:FragmentDefinition>